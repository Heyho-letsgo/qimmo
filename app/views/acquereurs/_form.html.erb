<%= semantic_form_for @acquereur do |f| %>
<%= f.inputs do %>
    <%= f.input :origine_rech %>
    <%= f.input :civilite %>
    <%= f.input :prenom %>
    <%= f.input :nom %>
    <%= f.input :num_rue %>
    <%= f.input :type_rue %>
    <%= f.input :adresse %>
    <%= f.input :cp %>
    <%= f.input :ville %>
    <%= f.input :tel_princ %>
    <%= f.input :tel_type %>
    <%= f.input :email_princ %>
    <%= f.input :email_type %>
    <%= f.input :fax_princ %>
    <%= f.input :r_cp %>
<% end %>

<%= f.actions do %>
<%= f.action :submit, :as => :input %>
<% end %>
<% end %>
